    <div data-options="region:'west',split:'true'" style="width: 230px">
        <div class="easyui-tabs" data-options="fit:true">
            <div title="往来单位">
                <ul class="easyui-tree" id="wldwda-tree-dw" data-options="
                  msg:'往来单位',
                  m:'SS_Customer',
                  associate:{
                    'wldwda-SS_Customer-GUID':['GUID'],
                    'wldwda-SS_Customer-CustomerKey':['CustomerKey'],
                    'wldwda-SS_Customer-CustomerName':['CustomerName'],
                    'wldwda-SS_Customer-CustomerAddress':['CustomerAddress'],
                    'wldwda-SS_Customer-CustomerBankName':['CustomerBankName'],
                    'wldwda-SS_Customer-CustomerBankNumber':['CustomerBankNumber'],
                    'wldwda-SS_Customer-CustomerTelephone':['CustomerTelephone'],
                    'wldwda-SS_Customer-CustomerLikeMan':['CustomerLikeMan'],
                    'wldwda-SS_Customer-CustomerFax':['CustomerFax'],
                    'wldwda-SS_Customer-CustomerPostcode':['CustomerPostcode'],
                    'wldwda-SS_Customer-CustomerWebsite':['CustomerWebsite'],
                    'wldwda-SS_Customer-IsVendor':['IsVendor'],
                    'wldwda-SS_Customer-IsCustomer':['IsCustomer']
                  },
