@{
    Layout = "~/Views/master/_Layout.cshtml";
    ViewBag.Title = "证件类型设置";
}
<script src="../../Scripts/jc/jcCommon.js" type="text/javascript"></script>
<div class="easyui-layout" id="zjlxsz-dataregion" data-options="fit:true" z="1">
    <div data-options="region:'north'" style="height: 51px">
        <div style="padding: 2px 0 2px 2px; background: #fafafa; border: 1px solid #ccc;">
                   <a href="#" class="easyui-linkbutton" 
                   id="zjlxsz-add" 
                   data-options="plain:'true',iconCls:'icon-xinzeng',
                   bindmethod:{ 'click':['newlyAdd'] },scope:'zjlxsz',status:'1',
                   forbidstatus:[1,2,3]">新增</a> 
                   
                   <a href="#" class="easyui-linkbutton"
                   id="zjlxsz-edit" data-options="plain:'true',iconCls:'icon-xiugai',
                   docState:'zjlxsz-SS_CredentialType-DocState',
                   bindmethod:{ 'click': ['setStatus'] },scope:'zjlxsz',status:'2',
                   forbidstatus:[1,2,3]">修改</a> 
                       
                   <a href="#" class="easyui-linkbutton" 
                   id="zjlxsz-remove" 
                   data-options="plain:'true',iconCls:'icon-shanchu',
                   docState:'zjlxsz-SS_CredentialType-DocState',
                   bindmethod:{ 'click': ['setStatus'] },scope:'zjlxsz',status:'3',
                   forbidstatus:[1,2,3]">删除</a>

                   <a href="#" class="easyui-linkbutton" id="zjlxsz-cancel" data-options="
                    plain:'true',iconCls:'icon-quxiao', scope:'zjlxsz',forbidstatus:[4],
                    bindmethod:{ 'click': ['cancel'] }">取消</a> 
                   
                   <a href="#" class="easyui-linkbutton" id="zjlxsz-save"
                   data-options="plain:'true',iconCls:'icon-baocun',
                   treeId:['zjlxsz-tree-Credential'],
                   bindmethod:{ 'click': ['saveDoc'] },
                   bindparms:{'saveDoc':['/JCqtsz/Save']},scope:'zjlxsz',status:'4',
                   forbidstatus:[4]">保存</a> 
                 
                   <a href="#" class="easyui-linkbutton"
                   id="zjlxsz-close" data-options="plain:'true',iconCls:'icon-tuichu',
                   bindmethod:{ 'click': ['closeTab'] },
                   scope:'zjlxsz'">退出</a>
        </div>
    </div>
    <div data-options="region:'west',split:'true'" style="width: 300px">
        <div class="easyui-tabs" data-options="fit:true">
            <div title="证件类型">
                <ul class="easyui-tree" id="zjlxsz-tree-Credential" 
                 data-options="associate:{
                    'zjlxsz-SS_CredentialType-GUID':['GUID'],
                    'zjlxsz-SS_CredentialType-CredentialTypekey':['CredentialTypekey'],
                    'zjlxsz-SS_CredentialType-CredentialTypeName':['CredentialTypeName'],
                    'zjlxsz-SS_CredentialType-IsStop':['IsStop']
                  },
                  bindmethod:{'onClick': ['setAssociate'] },
                  url:'/Tree/GetCredentialInfo',
                  customBindFirst:true,
                  findParent:false,
                  retStatus:'4',
                  method:'post'">
                </ul>
            </div>
        </div>
    </div>
    <div data-options="region:'center'" style="width: 600px" data-options="fit:true">
        <table border="0" style="height:auto; width: auto;padding: 0 0 0 150px">
            <tr>
                <td colspan="4" style="height: 100px;">
                    <div style="font-size: x-large; text-align: center;">
                        证&nbsp;件&nbsp;类&nbsp;型&nbsp;设&nbsp;置</div>
                </td>
            </tr>
            <tr style="height:50px;">
                <td>
                    <label for="field1" id="lbl-CredentialTypekey">
                        证件编号</label>
                </td>
                <td>
                    <input class="easyui-validatebox" id="zjlxsz-SS_CredentialType-CredentialTypekey" data-options="forbidstatus:[4]"/>
                </td>
                <td>
                    <label for="field1" id="lbl-CredentialTypeName">
                        证件名称</label>
                </td>
                <td>
                    <input class="easyui-validatebox" id="zjlxsz-SS_CredentialType-CredentialTypeName" data-options="forbidstatus:[4]"/>
                </td>
            </tr>
        </table>
    </div>
    <input class="easyui-validatebox" id="zjlxsz-SS_CredentialType-GUID" type="hidden"></input>
</div>
<label id="zjlxsz-extendregion" style="display: none">
    <input id="zjlxsz-status" type="text"/>
    <input id="initscope" type="text" value='zjlxsz'/>
    <input id="initstatus" type="text" value='4'/>
    <input id="initguid" type="text" value=''/>
</label>
<div id="b-window" line="true"></div>
