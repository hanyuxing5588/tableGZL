                    copyField:[
                        'lsggzd-BX_Detail-ProjectKey',
                        'lsggzd-BX_Detail-GUID_Project',
                        'lsggzd-BX_Detail-GUID_SettleType',
                        'lsggzd-CN_PaymentNumber-IsGuoKu',
                        'lsggzd-CN_PaymentNumber-IsProject',
                        'lsggzd-BX_Detail-GUID_Department',
                        'lsggzd-BX_Detail-GUID_BGType',
                        'lsggzd-CN_PaymentNumber-GUID_FunctionClass',
                        'lsggzd-CN_PaymentNumber-GUID_BGResource',
                        'lsggzd-BX_Detail-GUID_Person'
