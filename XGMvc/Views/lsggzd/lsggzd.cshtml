                    orderNum:'lsggzd-BX_Detail-OrderNum',
                    requireCol:[
                        'lsggzd-BX_Detail-GUID_BGCode',
                        'lsggzd-BX_Detail-Total_Real',
                        'lsggzd-BX_Detail-FeeMemo',
                        'lsggzd-CN_PaymentNumber-IsGuoKu',
                        'lsggzd-BX_Detail-GUID_Department',
                        'lsggzd-BX_Detail-GUID_SettleType',
                        'lsggzd-BX_Detail-GUID_BGType'
                    ],
                    fitColumns:false,
                    pagination:false,
                    method:'get',
                    singleSelect:true,
                    checkOnSelect:true,
                    height:260,
                    width:900,
                    rownumbers:true,
                    scope:'lsggzd',
                    single:false,
                    formatters:{'lsggzd-CN_PaymentNumber-IsGuoKu':'boolbox','lsggzd-BX_Detail-Total_Real':'numberbox'},
                    disableColMap:{
                            'lsggzd-CN_PaymentNumber-IsGuoKu':['lsggzd-CN_PaymentNumber-GUID_FunctionClass','lsggzd-BX_Detail-GUID_Project']
                    },
                    editEditAfterEvent:$.gridEdit,                    
                    payCodeConfig:['lsggzd','lsggzd-BX_Detail','lsggzd-CN_PaymentNumber'], @*scope = '', gridId = '', scopeModelName = '';*@
