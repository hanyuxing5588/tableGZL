                            <tr>
                                <th field="lsggzd-CN_PaymentNumber-GUID_ProjectEx" hidden="true"></th> @*附加项目*@
                                <th field="lsggzd-CN_PaymentNumber-ExtraCodeEx" hidden="true"></th> @*附加项目码*@
                                <th field="lsggzd-CN_PaymentNumber-ExtraCode" hidden="true"></th> @*附加码*@
                                <th field="lsggzd-CN_PaymentNumber-FinanceCode" hidden="true"></th> @*财政码*@
