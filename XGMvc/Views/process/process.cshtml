<script>
    $.xianshi = function (value1, row, index) {
        if (!value1) {
            return "开始";
        }
        return value1;
    };
</script>
<body>
    <div class="easyui-layout" id="process-dataregion" data-options="fit:true" z="1">
        <div data-options="region:'north'" style="height: 51px">
            <div style="padding: 2px 0 2px 2px; background: #fafafa; border: 1px solid #ccc;">
                <a href="#" class="easyui-linkbutton" id="process-edit" data-options="plain:'true',iconCls:'icon-queding',
                     bindmethod:{ 'click': ['cancelDetail'] },
                    bindparms:{'cancelDetail':['@ViewData["ModelUrl"]']},
                    scope:'process',          
                    window:'b-window',
                   scope:'parti',status:'0'">确定</a> 
                   </div>
        </div>
        <div data-options="region:'center',fit:true">
            <table border="0" width="100%">
                <tr>
                    <td colspan="6" style="width: 240px; height: 60px">
                        <div style="font-size: x-large; text-align: center;">
