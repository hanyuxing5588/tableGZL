                    </td>
                </tr>
                <tr>
                    <td colspan="6">
                       <table style="height:450px;padding:5px" id="@ViewData["ModelUrl"]-process" class="easyui-datagrid"  
                       b-type="1" 
                       data-options="
                        fitColumns:false,
                        method:'get',
                        singleSelect:true,
                        checkOnSelect:true,
                        pagination:true,
                        striped: false,
                        pageSize:20,
                        pageList:[20,50,100],
