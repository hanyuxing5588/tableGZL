<body>
    <div class="easyui-layout" id="parti-dataregion" data-options="fit:true" z="1">
        <div data-options="region:'north'" style="height: 51px">
            <div style="padding: 2px 0 2px 2px; background: #fafafa; border: 1px solid #ccc;">
                <a href="#" class="easyui-linkbutton" id="parti-sumbmit" 
                    data-options="plain:'true',iconCls:'icon-queding',
                    bindmethod:{ 'click': ['submitDetail'] },
                    bindparms:{'submitDetail':['parti','parti-CN_CashDetail']},
                    window:'b-window',
                    scope:'parti',status:'0',forbidstatus:[4]">确定</a> 
                    
                   <a href="#" class="easyui-linkbutton" id="parti-addrow" 
                   data-options="plain:'true',iconCls:'icon-zenghang',
                   bindmethod:{ 'click': ['detailAddRow'] },
                   bindparms:{'detailAddRow':['parti-CN_CashDetail']},
                   scope:'parti',forbidstatus:[4]">增行</a> 
                   
                   <a href="#" class="easyui-linkbutton" id="parti-InsertRow"
                   data-options="plain:'true',iconCls:'icon-chahang',
                   bindmethod:{ 'click': ['detailInsertRow'] },
                   bindparms:{'detailInsertRow':['parti-CN_CashDetail']},
                   scope:'parti',forbidstatus:[4]">插行</a> 
                   
                   <a href="#" class="easyui-linkbutton" id="parti-delrow"
                   data-options="plain:'true',iconCls:'icon-shanhang',
                   bindmethod:{ 'click': ['detailDeleteRow'] },
                   bindparms:{'detailDeleteRow':['parti-CN_CashDetail']},
                   scope:'parti',forbidstatus:[4]">删行</a> 
                   
                   <a href="#" class="easyui-linkbutton" id="parti-uprow"
                   data-options="plain:'true',iconCls:'icon-shangyitiao',
                   bindmethod:{ 'click': ['detailUpRow'] },
                   bindparms:{'delRow':['parti-CN_CashDetail']},
                   scope:'parti'">上一条</a> 
                   
                   <a href="#" class="easyui-linkbutton" id="parti-xiayih" 
                   data-options="plain:'true',iconCls:'icon-xiayitiao',
                   bindmethod:{ 'click': ['detailDownRow'] },
                   bindparms:{'delRow':['parti-CN_CashDetail']},
                   scope:'parti'">下一条</a> 
                        
                    <a href="#" class="easyui-linkbutton" id="parti-quxiao" 
                    data-options="plain:'true',iconCls:'icon-quxiao',
                    bindmethod:{ 'click': ['cancelDetail'] },
                    window:'b-window',
                    scope:'parti',status:'0'">取消</a>
            </div>
        </div>
        <div data-options="region:'center'" style="width: auto">
            <table border="0" style="height: 450px; width: 98%; margin: 5px">
                <tr>
                    <td colspan="8">
                        <div style="font-size: x-large; text-align: center;">
                            @ViewData["title"]</div>
                    </td>
                </tr>
                <tr>
                    <td style="width: 60px">
                        <label for="field1">
                            序&nbsp;&nbsp;&nbsp;&nbsp;号</label>
                    </td>
                    <td>
                        <input class="easyui-validatebox" id="parti-CN_CashDetail-OrderNum" data-options="forbidstatus:[-1]"
                            style="width: 137px"></input>
                    </td>
                    <td style="width: 60px">
                        <label for="field1">
                            金额</label>
                    </td>
                    <td>
                        <input class="easyui-numberbox" id="parti-CN_CashDetail-Total_Cash" data-options="precision:2,forbidstatus:[4],groupSeparator:',',max:99999999.99" style="width:137px"></input>
                    </td>
                    <td style="width: 60px">
                        <label>
                            财政支付令</label>
                    </td>
                    <td colspan="3">
                        <input class="easyui-validatebox" id="parti-CN_PaymentNumber-PaymentNumber" disabled="disabled"  style="width:350px" data-options="forbidstatus:[-1]"></input>
                    </td>
                </tr>
                <tr>
                    <td style="width: 60px">
                        <label>
                            支票号</label>
                    </td>
                    <td colspan="3">
                        <select class="easyui-combogrid" id="parti-CN_Check-GUID" 
                        data-options="
                            columns:[[
                                {field:'GUID',hidden:'true'},
                                {field:'BankAccountNo',title:'123',hidden:'true'},
                                {field:'GUID_BankAccount',title:'111',hidden:'true'},
                                {field:'BankAccountName',title:'银行账号名称',hidden:'true'},
                                {field:'CheckNumber',title:'支票号', width:335,height:150}
                               ]],
                            width:355,
                            panelWidth:355,
                            method:'post',
                            pagination:true,
                            pageSize:10,
                            pageList:[10,50,100],
                            appendcurcheckvalue:'1',   
                            remoteUrl:'/Grid/GetCheckListNotUseed',
                            idField:'GUID',
                            filterField:'BankAccountNo',
                            textField:'CheckNumber',
                            sortName:'CheckNumber',
                            bindmethod: { 'onCloseEx': ['setAssociate'] },
                            remoteSort:false,
                            forbidstatus:[4],
                            associate:
                              {
                                'parti-CN_CashDetail-BankAccountNo':['BankAccountNo','BankAccountNo'],
                                'parti-CN_CashDetail-GUID_BankAccount':['GUID_BankAccount','BankAccountName']
                              }
                     ">
                        </select>
                    </td>
                    <td style="width: 60px">
                        <label>
                            摘&nbsp;&nbsp;&nbsp;&nbsp;要</label>
                    </td>
                    <td colspan="3">
                        <input class="easyui-validatebox" id="parti-CN_CashDetail-CashMemo" style="width: 350px"
                            data-options="forbidstatus:[4]"></input>
                    </td>
                </tr>
                <tr>
                    <td style="width: 60px">
                        <label for="field1">
                            银行账号</label>
                    </td>
                    <td colspan="3">
                        <select class="easyui-combogrid" id="parti-CN_CashDetail-BankAccountNo" 
                        data-options="
                            columns:[[
                                {field:'GUID',hidden:'true'},
                                {field:'BankAccountNo',title:'银行账号',width:'175'},
                                {field:'BankAccountName',title:'银行账号名称',width:'150'}
                               ]],
                            width:350,
                            panelWidth:350,
                            method:'post',
                            remoteUrl:'/Combogrid/BankAccount',
                            idField:'BankAccountNo',
                            filterField:'BankAccountNo,BankAccountName',
                            textField:'BankAccountNo',
                            sortName:'BankAccountNo',
                           bindmethod: { 'onCloseEx': ['setAssociate'] },
                           remoteSort:false,
                           forbidstatus:[4],
                           associate:
                              {
                                'parti-CN_CashDetail-GUID_BankAccount':['GUID','BankAccountName']
                              }">
                        </select>
                    </td>
                    <td style="width: 60px">
                        <label>
                            银行名称</label>
                    </td>
                     <td colspan="3">
                        
                        <select class="easyui-combogrid" id="parti-CN_CashDetail-GUID_BankAccount" 
                         data-options="
                            columns:[[
                                {field:'GUID',hidden:'true'},
                                {field:'BankAccountNo',title:'银行账号',width:'175'},
                                {field:'BankAccountName',title:'银行账号名称',width:'150'}
                               ]],
                                width:350,
                                panelWidth:350,
                                method:'post',
                                remoteUrl:'/Combogrid/BankAccount',
                                idField:'GUID',
                                filterField:'BankAccountNo,BankAccountName',
                                textField:'BankAccountName',
                                bindmethod: { 'onCloseEx': ['setAssociate'] },
                                remoteSort:false,
                                forbidstatus:[4],
                                associate:
                              {
                                'parti-CN_CashDetail-BankAccountNo':['BankAccountNo','BankAccountNo']
                              }">
                        </select>
                    </td>
                </tr>
                <tr>
                    <td style="width: 60px">
                        <label>
                            项目名称</label>
                    </td>
                    <td colspan="3">
                        <select class="easyui-combogrid" id="parti-CN_CashDetail-GUID_Project" data-options="columns:[[
                            {field:'GUID',hidden:'true'},
                            {field:'ExtraCode',hidden:'true'},
                            {field:'FinanceProjectKey',hidden:'true'},
                            {field:'IsFinance',hidden:true},
                            {field:'IsProject',hidden:true},
                            {field:'GUID_FunctionClass',title:'GUID_FunctionClass',hidden:'true'},
                            {field:'ProjectKey',title:'项目编码',width:'110'},
                            {field:'ProjectName',title:'项目名称',width:'215'}]],width:350,panelWidth:350,method:'post',
                           remoteUrl:'/Combogrid/ProjectView',
                           CZZFField:'ExtraCode',
                           forbidstatus:[4],sortName:'ProjectKey',
                           idField:'GUID',textField:'ProjectName',
                            associate:
                            {
                                disableMap:{
                                     'parti-CN_PaymentNumber-GUID_FunctionClass':'true',
                                     'parti-CN_PaymentNumber-GUID_ProjectEx':'true',
                                     'parti-CN_PaymentNumber-IsGuoKu':'true',
                                     'parti-CN_PaymentNumber-IsProject':'true'
                                },
                                'parti-CN_PaymentNumber-GUID_FunctionClass':['GUID_FunctionClass'],
                                 'parti-CN_CashDetail-FinanceProjectKey':['FinanceProjectKey'],
                                 'parti-CN_PaymentNumber-IsGuoKu':['IsFinance'],
                                 'parti-CN_PaymentNumber-IsProject':['IsProject'],
                                 'parti-CN_CashDetail-ProjectKey':['ProjectKey','ProjectKey'],
                                 'parti-CN_PaymentNumber-ExtraCode':['ExtraCode'],
                                 'parti-CN_PaymentNumber-FinanceCode':['FinanceCode']
                            },
                           bindmethod: {'onCloseEx': ['setAssociate','setCCZFCode','setControlFun'] },
                            bindparms:{
                                'setCCZFCode':[
                                    'parti-CN_CashDetail-GUID_BGType',
                                    'parti-CN_PaymentNumber-GUID_FunctionClass',
                                    'parti-CN_PaymentNumber-GUID_EconomyClass',
                                    'parti-CN_PaymentNumber-GUID_ExpendType',
                                    'parti-CN_PaymentNumber-GUID_BGResource',
                                    'parti-CN_CashDetail-GUID_Project',
                                    'parti-CN_PaymentNumber-IsGuoKu',
                                    'parti-CN_PaymentNumber-PaymentNumber'],       
                                'setControlFun':{
                                    'parti-CN_PaymentNumber-GUID_FunctionClass':['combogrid','setAssociate'],
                                    'parti-CN_PaymentNumber-GUID_ProjectEx':['combogrid','clearValue'],
                                    'parti-CN_PaymentNumber-IsProject':['checkbox','setControlValue'],
                                    'parti-CN_PaymentNumber-IsGuoKu':['checkbox','setControlValue']
                            }},
                           filterField:'ProjectKey,ProjectName'
                         
                           "></select>
                    </td>
                    <td style="width: 60px">
                        <label>
                            项目编码</label>
                    </td>
                     <td colspan="3">
                        <select class="easyui-combogrid" id="parti-CN_CashDetail-ProjectKey" data-options="columns:[[
                            {field:'GUID',hidden:'true'},
                            {field:'ExtraCode',hidden:'true'},
                            {field:'FinanceProjectKey',hidden:'true'},
                            {field:'IsFinance',hidden:true},
                            {field:'IsProject',hidden:true},
                            {field:'GUID_FunctionClass',title:'GUID_FunctionClass',hidden:'true'},
                            {field:'ProjectKey',title:'项目编码',width:'110'},
                            {field:'ProjectName',title:'项目名称',width:'215'}
                            ]],width:350,panelWidth:350,method:'post',
                           remoteUrl:'/Combogrid/ProjectView',
                           CZZFField:'ExtraCode',
                           forbidstatus:[4],sortName:'ProjectKey',
                           idField:'ProjectKey',textField:'ProjectKey',
                            associate:
                            {
                                disableMap:{
                                     'parti-CN_PaymentNumber-GUID_ProjectEx':'true',
                                     'parti-CN_PaymentNumber-GUID_FunctionClass':'true',
                                     'parti-CN_PaymentNumber-IsGuoKu':'true',
                                     'parti-CN_PaymentNumber-IsProject':'true'
                                },
                                'parti-CN_PaymentNumber-GUID_FunctionClass':['GUID_FunctionClass'],
                                'parti-CN_CashDetail-FinanceProjectKey':['FinanceProjectKey'],
                                'parti-CN_PaymentNumber-IsGuoKu':['IsFinance'],
                                'parti-CN_PaymentNumber-IsProject':['IsProject'],
                                'parti-CN_CashDetail-GUID_Project':['GUID','ProjectName'],
                                'parti-CN_PaymentNumber-ExtraCode':['ExtraCode'],
                                'parti-CN_PaymentNumber-FinanceCode':['FinanceCode']
                            },
                            bindmethod: {'onCloseEx': ['setAssociate','setCCZFCode','setControlFun'] },
                            bindparms:{
                                'setCCZFCode':[
                                    'parti-CN_CashDetail-GUID_BGType',
                                    'parti-CN_PaymentNumber-GUID_FunctionClass',
                                    'parti-CN_PaymentNumber-GUID_EconomyClass',
                                    'parti-CN_PaymentNumber-GUID_ExpendType',
                                    'parti-CN_PaymentNumber-GUID_BGResource',
                                    'parti-CN_CashDetail-GUID_Project',
                                    'parti-CN_PaymentNumber-IsGuoKu',
                                    'parti-CN_PaymentNumber-PaymentNumber'
                                    ],       
                                'setControlFun':{
                                    'parti-CN_PaymentNumber-GUID_ProjectEx':['combogrid','clearValue'],
                                    'parti-CN_PaymentNumber-GUID_FunctionClass':['combogrid','setAssociate'],
                                    'parti-CN_PaymentNumber-IsProject':['checkbox','setControlValue'],
                                    'parti-CN_PaymentNumber-IsGuoKu':['checkbox','setControlValue']
                            }},
                           filterField:'ProjectKey,ProjectName'
                         
                           "/>
                    </td>
                </tr>
                <tr>
                    <td style="width: 60px">
                        <label>
                            功能分类</label>
                    </td>
                    <td colspan="3">
                        <select class="easyui-combogrid" id="parti-CN_PaymentNumber-GUID_FunctionClass" data-options="columns:[[
                            {field:'GUID',hidden:'true'},
                            {field:'FinanceCode',hidden:'true'},
                            {field:'IsProject',hidden:'true'},
                            {field:'IsFinance',hidden:'true'},
                            {field:'FunctionClassKey',title:'功能分类编码',width:'110'},
                            {field:'FunctionClassName',title:'功能分类名称',width:'215'}
                            ]],
                            associate:
                            {
                                disableMap:{
                                     'parti-CN_PaymentNumber-IsProject':'true'
                                },
                                'parti-CN_PaymentNumber-IsProject':['IsProject'], 
                                'parti-CN_PaymentNumber-FinanceCode':['FinanceCode'],                               
                                'parti-CN_PaymentNumber-BGType':['IsProject']

                            },
                           width:350,panelWidth:350,method:'post',
                           CZZFField:'FinanceCode',
                           remoteUrl:'/Combogrid/FunctionClass',
                           forbidstatus:[4],sortName:'FunctionClassKey',
                           idField:'GUID',textField:'FunctionClassName',
                            bindmethod:{ 'onCloseEx':['setAssociate','setCCZFCode','setIsGK','setControlFun'] },
                            bindparms:{
                                'setCCZFCode':[
                                    'parti-CN_CashDetail-GUID_BGType',
                                    'parti-CN_PaymentNumber-GUID_FunctionClass',
                                    'parti-CN_PaymentNumber-GUID_EconomyClass',
                                    'parti-CN_PaymentNumber-GUID_ExpendType',
                                    'parti-CN_PaymentNumber-GUID_BGResource',
                                    'parti-CN_CashDetail-GUID_Project',
                                    'parti-CN_PaymentNumber-IsGuoKu',
                                    'parti-CN_PaymentNumber-PaymentNumber'
                                ],
                                'setControlFun':{
                                    'parti-CN_PaymentNumber-IsProject':['checkbox','setControlValue']
                                },
                                'setIsGK':[
                                     'parti-CN_PaymentNumber-IsGuoKu'
                            ]},
                           filterField:'FunctionClassKey,FunctionClassName'"></select>
                    </td>
                      <td style="width: 60px">
                        <label>
                            是否项目</label>
                       </td>  
                    <td>
                        <input class="easyui-checkbox" id="parti-CN_PaymentNumber-IsProject" data-options="
     bindmethod:{ 'onClick':['setControlValue','setCCZFCode'] },
     forbidstatus:[4],
     bindparms:{
        'setControlValue':[
            'parti-CN_CashDetail-GUID_BGType'],
         'setCCZFCode':[
                'parti-CN_CashDetail-GUID_BGType',
                'parti-CN_PaymentNumber-GUID_FunctionClass',
                'parti-CN_PaymentNumber-GUID_EconomyClass',
                'parti-CN_PaymentNumber-GUID_ExpendType',
                'parti-CN_PaymentNumber-GUID_BGResource',
                'parti-CN_CashDetail-GUID_Project',
                'parti-CN_PaymentNumber-IsGuoKu',
                'parti-CN_PaymentNumber-PaymentNumber'

    ]}"  type="checkbox"/>
                    </td>
                    <td style="width: 60px">
                        <label for="field1">
                            是否国库</label>
                    </td>
                    <td>
                        <input class="easyui-checkbox" id="parti-CN_PaymentNumber-IsGuoKu" style="width: 80px" data-options="
     bindmethod:{ 'onClick':['setCCZFCode'] },
     forbidstatus:[4],
     bindparms:{
        'setCCZFCode':[
            'parti-CN_CashDetail-GUID_BGType',
            'parti-CN_PaymentNumber-GUID_FunctionClass',
            'parti-CN_PaymentNumber-GUID_EconomyClass',
            'parti-CN_PaymentNumber-GUID_ExpendType',
            'parti-CN_PaymentNumber-GUID_BGResource',
            'parti-CN_CashDetail-GUID_Project',
            'parti-CN_PaymentNumber-IsGuoKu',
            'parti-CN_PaymentNumber-PaymentNumber'

    ]}
    " type="checkbox"/></td>
                       
                    
                </tr>
                <tr>

                 <td style="width: 60px">
                        <label>
                            科目名称</label>
                    </td>
                    <td colspan="3">
                        <select class="easyui-combogrid" id="parti-CN_CashDetail-GUID_BGCode" data-options="columns:[[
                            {field:'GUID',hidden:'true'},
                            {field:'GUID_EconomyClass',hidden:'true'},
                            {field:'EconomyClassKey',hidden:'true'},
                            {field:'BGCodeKey',title:'科目编码',width:'110'},
                            {field:'BGCodeName',title:'科目名称',width:'215'}
                            ]],
                            width:350,panelWidth:350,method:'post',
                            associate:
                            {
                                disableMap:{
                                     'parti-CN_PaymentNumber-GUID_EconomyClass':'true'
                                },
                                'parti-CN_PaymentNumber-GUID_EconomyClass':['GUID_EconomyClass'],
                                'parti-CN_PaymentNumber-EconomyClassKey':['EconomyClassKey'],
                                'parti-CN_CashDetail-BGCodeKey':['GUID','BGCodeKey']
                            },
                           bindmethod: { 'onCloseEx': ['setAssociate','setControlFun'] },
                           bindparms:{
                           'setControlFun':{
                                    'parti-CN_PaymentNumber-GUID_FunctionClass':['combogrid','setCCZFCode']
                            }},
                           remoteUrl:'/Combogrid/BGCodeView',sortName:'BGCodeKey',
                           idField:'GUID',
                           textField:'BGCodeName',
                           forbidstatus:[4],
                           delay:1500,
                           filterField:'BGCodeKey,BGCodeName'">
                        </select>
                    </td>
                    <td style="width: 60px">
                        <label for="field1">
                            经济分类</label>
                    </td>
                    <td colspan="3">
                        <select class="easyui-combogrid" id="parti-CN_PaymentNumber-GUID_EconomyClass" 
                           data-options="columns:[[
                                   {field:'GUID',hidden:'true'},
                                   {field:'EconomyClassKey',title:'经济分类编码',width:'110'},
                                   {field:'EconomyClassName',title:'经济分类名称',width:'215'}
                                   ]],width:350,panelWidth:350,method:'post',
                           remoteUrl:'/Combogrid/EconomyClass',
                           CZZFField:'EconomyClassKey',
                           idField:'GUID',textField:'EconomyClassName',                         
                           bindmethod:{ 'onCloseEx':['setAssociate','setCCZFCode'] },
                           forbidstatus:[4],sortName:'EconomyClassKey',
                            bindparms:{
                                'setCCZFCode':[
                                    'parti-CN_CashDetail-GUID_BGType',
                                    'parti-CN_PaymentNumber-GUID_FunctionClass',
                                    'parti-CN_PaymentNumber-GUID_EconomyClass',
                                    'parti-CN_PaymentNumber-GUID_ExpendType',
                                    'parti-CN_PaymentNumber-GUID_BGResource',
                                    'parti-CN_CashDetail-GUID_Project',
                                    'parti-CN_PaymentNumber-IsGuoKu',
                                    'parti-CN_PaymentNumber-PaymentNumber'

                            ]},
                            associate: 
                            {
                             'parti-CN_PaymentNumber-EconomyClassKey':['EconomyClassKey'] 
                            },
                           filterField:'EconomyClassKey,EconomyClassName'"></select>
                    </td>
                   
                    
                </tr>
                <tr>
                    <td style="width: 60px">
                        <label for="field1">
                            预算来源</label>
                    </td>
                    <td colspan="3">
                        <select class="easyui-combogrid" id="parti-CN_PaymentNumber-GUID_BGResource" 
                           data-options="columns:[[
                                {field:'GUID',hidden:'true'},
                                {field:'BGSourceKey',title:'预算来源编码',width:'110'},
                                {field:'BGSourceName',title:'预算来源名称',width:'215'}
                                ]],width:350,panelWidth:350,method:'post',
                           remoteUrl:'/Combogrid/BgSource',
                            editable:false,
                            hideValue:'GUID_BGResource',sortName:'BGSourceKey',
                            bindmethod:{ 'onCloseEx':['setAssociate','setCCZFCode'] },
                            forbidstatus:[4],
                            bindparms:{
                                'setCCZFCode':[
                                    'parti-CN_CashDetail-GUID_BGType',
                                    'parti-CN_PaymentNumber-GUID_FunctionClass',
                                    'parti-CN_PaymentNumber-GUID_EconomyClass',
                                    'parti-CN_PaymentNumber-GUID_ExpendType',
                                    'parti-CN_PaymentNumber-GUID_BGResource',
                                    'parti-CN_CashDetail-GUID_Project',
                                    'parti-CN_PaymentNumber-IsGuoKu',
                                    'parti-CN_PaymentNumber-PaymentNumber'

                            ]},
                            associate:                            
                            {
                                'parti-CN_PaymentNumber-BGSourceKey':['BGSourceKey']
                            },
                           idField:'GUID',textField:'BGSourceName',
                           filterField:'BGSourceKey,BGSourceName',
                           CZZFField:'BGSourceKey'
                          "></select>
                    </td>
                    <td style="width: 60px">
                        <label>
                            支出类型</label>
                    </td>
                    <td colspan="3">
                        <select class="easyui-combogrid" id="parti-CN_PaymentNumber-GUID_ExpendType" 
                           data-options="columns:[[
                                {field:'GUID',hidden:'true'},
                                {field:'ExpendTypeKey',title:'支出类型编码',width:'110'},
                                {field:'ExpendTypeName',title:'支出类型名称',width:'215'}
                                ]],width:350,panelWidth:350,method:'post',
                           CZZFField:'ExpendTypeKey',
                           remoteUrl:'/Combogrid/ExpendType',
                            sortName:'ExpendTypeKey',
                           idField:'GUID',textField:'ExpendTypeName',
                           forbidstatus:[4],
                           bindmethod:{ 'onCloseEx':['setAssociate','setCCZFCode'] },
                           bindparms:{
                                'setCCZFCode':[
                                    'parti-CN_CashDetail-GUID_BGType',
                                    'parti-CN_PaymentNumber-GUID_FunctionClass',
                                    'parti-CN_PaymentNumber-GUID_EconomyClass',
                                    'parti-CN_PaymentNumber-GUID_ExpendType',
                                    'parti-CN_PaymentNumber-GUID_BGResource',
                                    'parti-CN_CashDetail-GUID_Project',
                                    'parti-CN_PaymentNumber-IsGuoKu',
                                    'parti-CN_PaymentNumber-PaymentNumber'

                            ]},
                            associate:
                            {
                            'parti-CN_PaymentNumber-ExpendTypeKey':['ExpendTypeKey']
                            },
                           filterField:'ExpendTypeKey,ExpendTypeName'"></select>
                    </td>
                </tr>
                 <tr>
                    <td style="width: 60px">
                        <label for="field1">
                            项目辅助编码</label>
                    </td>
                    <td colspan="3">
                        <select class="easyui-combogrid" id="parti-CN_PaymentNumber-GUID_ProjectEx" data-options="columns:[[
                               {field:'GUID',hidden:'true'},
                            {field:'ExtraCode',hidden:'true'},
                              {field:'IsProject',hidden:'true'},
                            {field:'FinanceCode',hidden:'true'},
                            {field:'FinanceProjectKey',hidden:'true'},
                            {field:'IsFinance',hidden:true},
                            {field:'GUID_FunctionClass',title:'GUID_FunctionClass',hidden:'true'},

                            {field:'ProjectKey',title:'项目编码',width:'110'},
                            {field:'ProjectName',title:'项目名称',width:'215'}]],width:350,panelWidth:350,method:'post',
                           remoteUrl:'/Combogrid/ProjectViewFX',
                           CZZFField:'ExtraCode',
                           forbidstatus:[4],sortName:'ProjectKey',
                           idField:'GUID',textField:'ProjectName',
                            associate:
                            {
                                'parti-CN_PaymentNumber-ExtraCodeEx':['ExtraCode']
                            },
