        //用户退出        function logonOut() {
            $.ajax({
                url: '/Logon/LogonOut',
                cache: false,
                async: true,
                type: 'post',
                datatype: 'Text',
                success: function (data) {

                    if (data == "ok") {
                        win_Close();
                    }
                }
            });
        }
        //关闭当前页面
        var win_Close = function () {
            if (confirm("您确定要退出本系统吗？")) {
                window.opener = null;
                window.open('', '_self');
                window.close();
            }
        }

        //用户注销
        function logonCancle() {
            $.ajax({
